<schemaMeta>
<tables>
	<!--
	Set relation between two columns
	ChildTable.ChildCol -> ParentTable.ParentCol
	-->
	<table  name="ChildTable">
	<column name="ChildCol">
	<foreignKey table="ParentTable"
				column="ParentCol">
	</foreignKey>
	</column>
	</table>

	<!--
	Set table description
	-->
	<table name="TableName" comments="Table description">
	</table>

</tables>
</schemaMeta>